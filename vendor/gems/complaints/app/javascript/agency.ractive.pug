span.agency
  | {{name}}

script.
  import Ractive from 'ractive'
  export default Ractive.extend({
    template : $TEMPLATE,
    computed : {
      name() {
        const agency = _(window.complaints_page_data().all_agencies).findWhere({id : this.get('id')});
        if (_.isUndefined(agency)) { return null; } else { return agency.name; }
      }
    }
  });

