%div#detailsContent{:style=>"display: none"}
  %table{:style => 'width:500px'}
    %tr
      %td Original file name
      %td {%= o.original_filename %}
    %tr
      %td File size
      %td {%= o.formatted_filesize %}
    %tr
      %td Revision
      %td {%= o.revision %}
    %tr
      %td Last modified
      %td {%= o.lastmodifieddate %}
    %tr
      %td Uploaded on
      %td {%= o.created_at %}
